    "@babel/types": "^7.12.12",
    "@types/chai": "^4.2.11",
    "@types/jsdom": "^16.2.3",
    "@types/mocha": "^8.0.0",
    "@types/sinon": "^9.0.4",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "chai": "^4.2.0",
    "d3-scale-chromatic": "^2.0.0",
    "jsdom": "^16.3.0",
    "mocha": "^8.0.1",
    "perf-monitor": "^0.4.1",
    "sinon": "^9.0.2"
